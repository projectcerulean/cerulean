"language": &"GDScript",
"path": "res://src/entities/player/player_state.gd"
}, {
"base": "Node",
"class": &"State",
"language": &"GDScript",
"path": "res://src/common/finite_state_machine/state.gd"
}, {
"base": "Node",
"class": &"StateMachine",
